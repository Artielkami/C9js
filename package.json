{
  "name": "C9js",
  "version": "0.0.0-semantically-released",
  "main": "src/index.js",
  "dependencies": {},
  "scripts": {
    "commit": "git-cz",
    "test": "mocha test/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js node_modules/mocha/bin/_mocha -- -R spec test/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "api",
    "js",
    "library",
    "map",
    "chart",
    "d3",
    "openlayer",
    "leaflet",
    "interactive",
    "data"
  ],
  "description": "A JavaScript library, used for interactive map-chart based on various types of data.",
  "devDependencies": {
    "chai": "3.5.0",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "1.3.2",
    "istanbul": "0.4.5",
    "mocha": "3.1.0",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/csethanhcong/C9js.git"
